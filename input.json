{
  "output_format": {
    "destination": "csv",
    "filename": "users.csv"
  },
  "collections": [
    {
      "name": "person",
      "size": 1000,
      "attributes": [
        {
          "name": "pin",
          "attribute_type": "expression",
          "value": "[1-9]{6}"
        },
        {
          "name": "password",
          "attribute_type": "expression",
          "value": "[A-Za-z0-9]{16}"
        },
        {
          "name": "first_name",
          "attribute_type": "lookup",
          "value": "names.first_name"
        },
        {
          "name": "last_name",
          "attribute_type": "lookup",
          "value": "names.last_name"
        },
        {
          "name": "ssn",
          "attribute_type": "expression",
          "value": "[1-8]{1}[0-9]{2}[\\-]{1}[0-9]{2}[\\-]{1}[0-9]{4}"
        },
        {
          "name": "age",
          "attribute_type": "expression",
          "value": "[1-8]{1}[0-9]{1}"
        },
        {
          "name": "phone_no",
          "attribute_type": "expression",
          "value": "[5]{1}[8]{1}[5]{1}[\\-]{1}[0-9]{3}[(]{1}[0-9]{4}[)]{1}"
        }
      ]
    }
  ]
}